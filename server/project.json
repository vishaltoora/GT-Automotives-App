{
  "name": "server",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "server/src",
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/server",
        "main": "server/src/main.ts",
        "tsConfig": "server/tsconfig.app.json",
        "assets": ["server/src/assets"],
        "compiler": "tsc",
        "target": "node",
        "webpackConfig": "server/webpack.config.js"
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "inspect": false,
          "fileReplacements": [
            {
              "replace": "server/src/environments/environment.ts",
              "with": "server/src/environments/environment.prod.ts"
            }
          ]
        },
        "development": {
          "optimization": false,
          "extractLicenses": false,
          "inspect": true
        }
      }
    },
    "serve": {
      "executor": "@nx/js:node",
      "defaultConfiguration": "development",
      "dependsOn": ["build"],
      "options": {
        "buildTarget": "server:build",
        "runBuildTargetDependencies": false
      },
      "configurations": {
        "development": {
          "buildTarget": "server:build:development"
        },
        "production": {
          "buildTarget": "server:build:production"
        }
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "cache": true,
      "dependsOn": ["^typecheck"],
      "inputs": [
        "{projectRoot}/package.json",
        "{workspaceRoot}/tsconfig.base.json",
        "{projectRoot}/tsconfig.json",
        "{projectRoot}/tsconfig.app.json",
        "{projectRoot}/src/**/*.ts",
        {
          "dependentTasksOutputFiles": "**/*.d.ts"
        },
        {
          "externalDependencies": ["typescript"]
        }
      ],
      "outputs": [
        "{projectRoot}/dist/**/*.d.ts",
        "{projectRoot}/dist/**/*.d.ts.map",
        "{projectRoot}/dist/tsconfig.app.tsbuildinfo"
      ],
      "options": {
        "cwd": "server",
        "command": "tsc --build --emitDeclarationOnly"
      }
    }
  },
  "tags": ["npm:private"]
}