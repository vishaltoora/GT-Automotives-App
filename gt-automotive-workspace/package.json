{
  "name": "@gt-automotive-workspace/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "nx run-many --target=serve --projects=webApp,server --parallel",
    "dev:web": "nx serve webApp",
    "dev:server": "nx serve server",
    "build": "nx run-many --target=build --all",
    "build:web": "nx build webApp",
    "build:server": "nx build server",
    "test": "nx run-many --target=test --all",
    "test:web": "nx test webApp",
    "test:server": "nx test server",
    "lint": "nx run-many --target=lint --all",
    "lint:fix": "nx run-many --target=lint --all --fix",
    "db:generate": "prisma generate --schema=libs/database/src/lib/prisma/schema.prisma",
    "db:migrate": "prisma migrate dev --schema=libs/database/src/lib/prisma/schema.prisma",
    "db:migrate:prod": "prisma migrate deploy --schema=libs/database/src/lib/prisma/schema.prisma",
    "db:reset": "prisma migrate reset --schema=libs/database/src/lib/prisma/schema.prisma",
    "db:seed": "yarn ts-node libs/database/src/lib/prisma/seed.ts",
    "db:studio": "prisma studio --schema=libs/database/src/lib/prisma/schema.prisma",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f"
  },
  "private": true,
  "devDependencies": {
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.0",
    "@nx/eslint": "^21.3.11",
    "@nx/jest": "^21.3.11",
    "@nx/js": "^21.3.11",
    "@nx/nest": "^21.3.11",
    "@nx/node": "^21.3.11",
    "@nx/react": "^21.3.11",
    "@nx/vite": "21.3.11",
    "@nx/web": "^21.3.11",
    "@nx/webpack": "21.3.11",
    "@nx/workspace": "^21.3.11",
    "@prisma/client": "^6.14.0",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@testing-library/dom": "10.4.0",
    "@testing-library/react": "16.1.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.0.0",
    "@types/react": "19.0.0",
    "@types/react-dom": "19.0.0",
    "@vitejs/plugin-react": "^4.2.0",
    "@vitest/ui": "^3.0.0",
    "babel-jest": "^30.0.2",
    "jest": "^30.0.2",
    "jest-environment-jsdom": "^30.0.2",
    "jest-util": "^30.0.2",
    "jiti": "2.4.2",
    "jsdom": "~22.1.0",
    "nx": "21.3.11",
    "prettier": "^2.6.2",
    "prisma": "^6.14.0",
    "ts-jest": "^29.4.0",
    "ts-node": "10.9.1",
    "tslib": "^2.3.0",
    "typescript": "~5.8.2",
    "vite": "^6.0.0",
    "vitest": "^3.0.0",
    "webpack-cli": "^5.1.4"
  },
  "workspaces": [
    "packages/*",
    "apps/*",
    "server",
    "server-e2e",
    "libs/shared/*",
    "libs/*"
  ],
  "dependencies": {
    "@clerk/backend": "^2.7.1",
    "@clerk/clerk-react": "^5.41.0",
    "@clerk/clerk-sdk-node": "^5.1.6",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@hookform/resolvers": "^5.2.1",
    "@mui/icons-material": "^7.3.1",
    "@mui/material": "^7.3.1",
    "@mui/x-date-pickers": "^8.10.0",
    "@nestjs/common": "^11.0.0",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.0",
    "@nestjs/platform-express": "^11.0.0",
    "@tanstack/react-query": "^5.85.0",
    "axios": "^1.6.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "^7.62.0",
    "react-router-dom": "6.29.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0",
    "yup": "^1.7.0"
  },
  "prisma": {
    "seed": "ts-node libs/database/src/lib/prisma/seed.ts"
  }
}
